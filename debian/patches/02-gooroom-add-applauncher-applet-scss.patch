diff --git a/common/gtk-3.0/3.20/sass/_gooroom-applauncher-applet.scss b/common/gtk-3.0/3.20/sass/_gooroom-applauncher-applet.scss
new file mode 100644
index 0000000..a6ccb50
--- /dev/null
+++ b/common/gtk-3.0/3.20/sass/_gooroom-applauncher-applet.scss
@@ -0,0 +1,121 @@
+* {
+  -gtk-icon-shadow: none;
+  -gtk-icon-effect: none;
+  outline-style: none;
+  transition: none;
+  animation: none;
+  text-shadow: none;
+  box-shadow: none;
+}
+
+tooltip {
+  border-radius: 5px; border: none;
+
+  &.background {
+    background-clip: padding-box;
+    background-color: transparent;
+    background-image: image(rgba(255, 255, 255, 0.95));
+  }
+
+  decoration { background-color: transparent; }
+
+  * {
+    padding: 0;
+    background-color: transparent;
+    color: #373737;
+  }
+}
+
+#GooroomApplauncherWindow {
+  #directory-item-box {
+    min-width: 300px;
+    min-height: 400px;
+    border: none;
+    border-radius: 8px;
+    border-image: none;
+    background-color: rgba(0, 0, 0, 0.85);
+  }
+
+  #app-item-box {
+    border: none;
+    border-radius: 8px;
+    border-image: none;
+    background-color: rgba(0, 0, 0, 0.85);
+  }
+
+  #search-entry {
+    min-height: 34px;
+    border: 1px solid transparent;
+    padding: 5px;
+    border-image: radial-gradient(circle farthest-corner at center, rgba(255, 255, 255, 0.5) 100%, transparent) 0 0 1/0 0 1px;
+    background-color: transparent;
+    caret-color: #ffffff;
+    color: #ffffff;
+
+    image {
+      color: #ffffff;
+
+      &.left { margin-left: 0; margin-right: 10px; }
+      &.right { margin-left: 10px; margin-right: 0; }
+    }
+
+    &:focus {
+      border-image: radial-gradient(circle farthest-corner at center, #3986e1 100%, transparent) 0 0 1/0 0 1px;
+      color: #ffffff;
+      background-image: none;
+      background-color: transparent;
+    }
+  }
+
+  #directory-item {
+    border: 1px solid transparent;
+    padding: 5px 10px;
+    color: #ffffff;
+    font-size: 105%;
+    font-weight: bold;
+    background-image: none;
+    background-clip: padding-box;
+    background-color: transparent;
+
+    &:hover { background-color: rgba(0, 188, 212, 0.85); }
+
+    &:checked { background-color: rgba(74, 144, 226, 0.85); }
+  }
+
+  #appitem {
+    border: 1px solid transparent;
+    border-radius: 0;
+    padding: 0;
+    color: #ffffff;
+    background-image: none;
+    background-color: transparent;
+
+    &:hover {
+      border: 1px solid rgba(0, 188, 212, 0.9);
+      background-color: rgba(0, 188, 212, 0.2);
+    }
+
+    &:focus,
+    &:active {
+      border: 1px solid rgba(0, 188, 212, 0.6);
+      background-color: rgba(0, 188, 212, 0.6);
+    }
+  }
+
+  #indicator-button {
+    border: none;
+    padding: 0;
+    color: rgba(255, 255, 255, 0.3);
+    background-image: none;
+    background-color: transparent;
+
+    &:hover {
+      color: rgba(255, 255, 255, 1.0);
+    }
+
+    &:active,
+    &:checked {
+      color: #50e3c2;
+    }
+  }
+}
