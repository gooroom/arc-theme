diff --git a/common/gtk-3.0/3.20/sass/_gooroom-integration-applet.scss b/common/gtk-3.0/3.20/sass/_gooroom-integration-applet.scss
new file mode 100644
index 0000000..7dd4a7f
--- /dev/null
+++ b/common/gtk-3.0/3.20/sass/_gooroom-integration-applet.scss
@@ -0,0 +1,108 @@
+* {
+  -gtk-icon-shadow: none;
+  -gtk-icon-effect: none;
+  outline-style: none;
+  transition: none;
+  animation: none;
+  text-shadow: none;
+  box-shadow: none;
+}
+
+#GooroomIntegrationWindow {
+  #main-box {
+    min-width: 420px;
+    min-height:360px;
+    padding: 30px 20px;
+    border: none;
+    border-radius: 8px;
+    border-image: none;
+    background-color: rgba(0, 0, 0, 0.85);
+  }
+
+  separator { background: rgba(255, 255, 255, 0.1); }
+
+  #module-widget {
+    label, image { color: inherit; }
+
+    border: 1px solid transparent;
+    padding: 10px 8px 10px 7px;
+    color: #ffffff;
+    background-image: none;
+    background-clip: padding-box;
+    background-color: transparent;
+
+    &:hover {
+      background-color: rgba(0, 188, 212, 1.0);
+    }
+
+    &:active {
+      background-color: rgba(74, 144, 226, 1.0);
+    }
+    &:disabled {
+      background-color: transparent;
+      color: rgba(255, 255, 255, 0.3);
+    }
+  }
+
+  #icon-button {
+    border: none;
+    padding: 0;
+    color: #ffffff;
+    background-image: none;
+    background-color: transparent;
+
+    &:hover {
+      color: rgba(0, 188, 212, 1.0);
+    }
+
+    &:active {
+      color: rgba(74, 144, 226, 1.0);
+    }
+
+    &:disabled {
+      color: rgba(255, 255, 255, 0.2);
+    }
+  }
+
+  //
+  // Calendar
+  //
+  calendar {
+    color: #ffffff;
+    border: none;
+    border-top: 1px solid rgba(255, 255, 255, 0.20);
+    border-bottom: 1px solid rgba(255, 255, 255, 0.20);
+    padding: 6px;
+  
+    &.view {
+      background-color: transparent;
+    }
+  
+    &:selected {
+      color: #ff0000;
+      border: none;
+      background-color: transparent;
+    }
+  
+    &.header {
+      color: #ffffff;
+      border: none;
+      background-color: transparent;
+    }
+  
+    &.button,
+    &.header.button {
+      @extend %undecorated_button;
+  
+      color: #ffffff;
+      background-color: transparent;
+  
+      &:hover { color: rgba(255, 255, 255, 0.55); }
+  
+      &:disabled { color: rgba(255, 255, 255, 0.30); }
+    }
+  
+    &:indeterminate { color: rgba(255, 255, 255, 0.30); }
+    &.highlight { color: rgba(255, 255, 255, 0.85); }
+  }
+}
